@model IEnumerable<cde.district.validation.Result>

@{
    ViewBag.Title = "Finish";
}

<h2>Validation Results (@ViewBag.TotalErrors issues)</h2>

<p>Test Totals
	<ul>
		@foreach (var tuple in (IEnumerable<Tuple<string, int>>)ViewBag.TestTotals) {
			<li><b>@tuple.Item1:</b> @tuple.Item2</li>
  }
	</ul>
</p>

@foreach (var item in Model) {
<p>
	<h3>@item.Title</h3><br />
	@foreach(var error in item.Errors) {
		@error <br />
	}
</p>
}